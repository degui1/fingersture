<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="Fingersture.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:Fingersture"
       Shell.FlyoutBehavior="Disabled"
       Title="Fingersture">

    <ShellContent Title="Authentication"
                  ContentTemplate="{DataTemplate local:Authentication}"
                  Route="Authentication" />

    <ShellContent Title="Home"
                  ContentTemplate="{DataTemplate local:Home}"
                  Route="Home" />

    <ShellContent Title="SignUp"
                  ContentTemplate="{DataTemplate local:SignUp}"
                  Route="SignUp" />

    <ShellContent Title="SignIn"
                  ContentTemplate="{DataTemplate local:SignIn}"
                  Route="SignIn" />

</Shell>
