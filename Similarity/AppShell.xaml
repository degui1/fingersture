<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="Similarity.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:Similarity"
       Shell.FlyoutBehavior="Disabled"
       Title="Similarity">

    <ShellContent Title="Home"
                  ContentTemplate="{DataTemplate local:MainPage}"
                  Route="MainPage" />

    <ShellContent Title="AcessoLiberado"
                  ContentTemplate="{DataTemplate local:AcessoLiberado}"
                  Route="AcessoLiberado" />

    <ShellContent Title="Cadastro"
                  ContentTemplate="{DataTemplate local:Cadastro}"
                  Route="Cadastro" />

    <ShellContent Title="Login"
                  ContentTemplate="{DataTemplate local:Login}"
                  Route="Login" />

</Shell>
